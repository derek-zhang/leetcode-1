//第i天买入，能赚的利润是i+1 ~ n中最大股价- 第i天股价
int maxProfit(vector<int> &prices)
{
  if(prices.size() == 0)
		return 0;
	int maxPrice = prices[prices.size() - 1];
	int ret = 0;
	for(int i = prices.size() - 1; i >= 0; i--)
	{
		maxPrice = max(maxPrice, prices[i]);
		ret = max(ret, maxPrice - prices[i]);
	}
	return ret;
}
