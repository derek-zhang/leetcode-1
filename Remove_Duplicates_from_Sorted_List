struct ListNode {
  int val;
	ListNode *next;
	ListNode(int x) : val(x), next(NULL) {}
};

ListNode *deleteDuplicates(ListNode *head)
{
	if(head == NULL)
		return NULL;
	ListNode *pPre = head;
	ListNode *pCur = pPre->next;
	ListNode *del = NULL;
	while(pCur)
	{
		if(pCur->val == pPre->val)
		{
			del = pCur;
			pPre->next = pCur->next;
			pCur = pCur->next;
			delete del;
		}
		else
		{
			pCur = pCur->next;
			pPre = pPre->next;
		}	
	}
	return head;
}
